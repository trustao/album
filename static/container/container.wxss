/* components/container.wxss */
page{
  width: 100%;
  height: 100%;
}

.container {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding-top: 128rpx;
  overflow: hidden;
}
.container.iphoneX {
  padding-top: 176rpx;
}
.container.plus {
  padding-top: 116rpx;
}
.container.iphone5 {
  padding-top: 142rpx;
}
.container.no-header {
  padding-top: 0!important;
}
.container .header{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 128rpx;
    background: #fff;
    box-shadow: 0 0 2px 0 #C6C6C6;
}
.container.plus .header{
  height: 116rpx;
}
.container.iphoneX .header{
  height: 176rpx;
}
.container.iphone5 .header{
  height: 142rpx;
}
.container .header .header-cvs{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.container .header .back{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 88rpx;
  height: 88rpx;
}
.container .header .back .back-img{
  position: absolute;
  left: 50%;
  bottom: 16rpx;
  width: 50rpx;
  height: 50rpx;
  transform: translateX(-50%);
  z-index: 9;
}
.container .header .title{
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  font-size: 34rpx;
  font-weight: 700;
  transform: translateX(-50%);
  z-index: 9;
}
.container.iphone5 .header .title{
  font-size: 38rpx;
}
.container .header .back.no{
  display: none;
}
.container.iphoneX .header {
  height: 176rpx;
}
.container .main{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  background: #f4f4f4;
}
